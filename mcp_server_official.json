    {
      "name": "naive_retriever",
      "description": "Invokes the Naive Retriever chain for basic similarity search.",
      "inputSchema": {
        "type": "object",
        "properties": {
          "question": {
            "type": "string",
            "title": "Question"
          }
        },
        "required": [
          "question"
        ]
      },
      "annotations": {
        "audience": [
          "human",
          "llm"
        ],
        "cachePolicy": {
          "ttl": 300
        },
        "governance": {
          "dataAccess": "public",
          "aiEnabled": true,
          "category": "search",
          "requiresApproval": false,
          "isReadOnly": true,
          "isDestructive": false,
          "hasNetworkAccess": true,
          "dataClassification": "public"
        },
        "resources": {
          "isIntensive": false,
          "estimatedDuration": "medium",
          "memoryUsage": "low",
          "networkUsage": "medium",
          "storageAccess": "read"
        },
        "trustAndSafety": {
          "contentFiltering": true,
          "rateLimited": true,
          "auditLogged": true,
          "requiresHumanInLoop": false
        }
      },
      "examples": [
        {
          "id": "naive_retriever_example_1",
          "description": "Example naive retriever search for: What makes a good action movie?",
          "input": {
            "question": "What makes a good action movie?"
          },
          "output": {
            "contentTypes": [
              "text"
            ],
            "schema": {
              "type": "object",
              "properties": {
                "content": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": [
                          "text"
                        ]
                      },
                      "text": {
                        "type": "string"
                      }
                    }
                  }
                },
                "isError": {
                  "type": "boolean"
                }
              }
            },
            "description": "Returns relevant movie review excerpts about what makes a good action movie?"
          }
        },
        {
          "id": "naive_retriever_example_2",
          "description": "Example naive retriever search for: How does John Wick compare to other action heroes?",
          "input": {
            "question": "How does John Wick compare to other action heroes?"
          },
          "output": {
            "contentTypes": [
              "text"
            ],
            "schema": {
              "type": "object",
              "properties": {
                "content": {
                  "type": "array",
